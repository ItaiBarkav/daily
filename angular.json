{
  "version": 1,
  "projects": {
    "daily": {
      "projectType": "application",
      "root": "apps/daily",
      "sourceRoot": "apps/daily/src",
      "prefix": "manage-tool",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/daily",
            "index": "apps/daily/src/index.html",
            "main": "apps/daily/src/main.ts",
            "polyfills": "apps/daily/src/polyfills.ts",
            "tsConfig": "apps/daily/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/daily/src/favicon.ico",
              "apps/daily/src/assets",
              {
                "input": "libs/ui/src/assets",
                "glob": "**/*",
                "output": "assets/ui"
              }
            ],
            "styles": ["apps/daily/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/daily/src/environments/environment.ts",
                  "with": "apps/daily/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "daily:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "daily:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "daily:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/daily/tsconfig.app.json",
              "apps/daily/tsconfig.spec.json",
              "apps/daily/tsconfig.editor.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/daily/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/daily"],
          "options": {
            "jestConfig": "apps/daily/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "daily-e2e": {
      "root": "apps/daily-e2e",
      "sourceRoot": "apps/daily-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/daily-e2e/cypress.json",
            "tsConfig": "apps/daily-e2e/tsconfig.e2e.json",
            "devServerTarget": "daily:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "daily:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/daily-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/daily-e2e/**/*"]
          }
        }
      }
    },
    "material": {
      "projectType": "library",
      "root": "libs/material",
      "sourceRoot": "libs/material/src",
      "prefix": "manage-tool",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/material/tsconfig.lib.json",
              "libs/material/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/material/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/material"],
          "options": {
            "jestConfig": "libs/material/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ui": {
      "projectType": "library",
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "prefix": "manage-tool",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/ui/tsconfig.lib.json",
              "libs/ui/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/ui/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui"],
          "options": {
            "jestConfig": "libs/ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "models": {
      "root": "libs/models",
      "sourceRoot": "libs/models/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/models/tsconfig.lib.json",
              "libs/models/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/models/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/models"],
          "options": {
            "jestConfig": "libs/models/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "electron": {
      "root": "apps/electron",
      "sourceRoot": "apps/electron/src",
      "projectType": "application",
      "prefix": "electron",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "nx-electron:build",
          "options": {
            "outputPath": "dist/apps/electron",
            "main": "apps/electron/src/main.ts",
            "tsConfig": "apps/electron/tsconfig.app.json",
            "assets": ["apps/electron/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/electron/src/environments/environment.ts",
                  "with": "apps/electron/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "nx-electron:execute",
          "options": {
            "buildTarget": "electron:build"
          }
        },
        "package": {
          "builder": "nx-electron:package",
          "options": {
            "name": "electron",
            "frontendProject": "daily",
            "outputPath": "dist/packages",
            "prepackageOnly": true
          }
        },
        "make": {
          "builder": "nx-electron:make",
          "options": {
            "name": "electron",
            "frontendProject": "daily",
            "outputPath": "dist/executables"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/electron/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/electron/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "tslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "daily"
}
